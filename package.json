  "devDependencies": {
    "@parcel/transformer-sass": "2.6.0",
    "autoprefixer": "^10.4.7",
    "parcel": "^2.6.0",
    "parcel-reporter-static-files-copy": "^1.3.4",
    "postcss": "^8.4.14",
    "push-dir": "^0.4.1",
    "shx": "^0.3.4",
    "tailwindcss": "^3.0.24"
  },
  "@parcel/transformer-css": {
